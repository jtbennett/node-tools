  "devDependencies": {
    "@types/async": "^3.2.3",
    "@types/lodash": "^4.14.154",
    "@types/moment": "^2.13.0",
    "@types/mongodb": "^3.5.20",
    "@types/node": "^12.7.8",
    "eslint": "^6.8.0",
    "jest": "24.9.0",
    "mongodb": "^3.5.8",
    "rimraf": "^3.0.0",
    "ts-essentials": "^6.0.5",
    "typescript": "~3.9.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
